{
  "name": "whisp-stamp",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "deps:check": "npx npm-check-updates",
    "deps:update": "npx npm-check-updates -u && npm install",
    "deploy": "npm run build && npx wrangler deploy",
    "deploy:dry-run": "npm run build && npx wrangler deploy --dry-run",
    "deploy:dev": "npx wrangler dev",
    "deploy:pages": "npm run build && npx wrangler pages deploy .svelte-kit/cloudflare"
  },
  "devDependencies": {
    "@sveltejs/adapter-cloudflare": "^7.2.4",
    "@sveltejs/kit": "^2.48.5",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "happy-dom": "^20.0.10",
    "npm-check-updates": "^19.1.2",
    "svelte": "^5.43.6",
    "svelte-check": "^4.3.4",
    "typescript": "^5.9.3",
    "vite": "^7.2.2",
    "vitest": "^4.0.9",
    "wrangler": "^4.47.0"
  }
}